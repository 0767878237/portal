<odoo>
  <template id="portal_my_account" inherit_id="portal.portal_my_account">
    <xpath expr="//div[@id='wrap']" position="replace">
      <t t-call="portal.portal_layout">
        <div class="container" id="wrap">
          <h2>Your Information</h2>
          <ul class="list-unstyled">
            <li><strong>Name:</strong> <t t-esc="partner.name"/></li>
            <li><strong>Email:</strong> <t t-esc="partner.email"/></li>
            <li><strong>Phone:</strong> <t t-esc="partner.phone"/></li>
          </ul>
        </div>
      </t>
    </xpath>
  </template>
</odoo>
